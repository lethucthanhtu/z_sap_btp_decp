{
  "name": "z_sap_btp_decp",
  "version": "0.0.1",
  "description": "An SAPUI5 app to visualise warehouse floorplans in 3D, showing real-time asset locations for improved operational visibility and space management.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.7",
    "@sap-ux/eslint-plugin-fiori-tools": "^0.4.0",
    "@sap/ux-ui5-tooling": "1",
    "@sapui5/types": "~1.138.0",
    "@types/node": "^24.2.0",
    "@types/qunit": "^2.19.12",
    "@types/three": "^0.179.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@ui5/cli": "^4.0.16",
    "eslint": "8.57.0",
    "eslint-plugin-fiori-custom": "2.6.7",
    "typescript": "^5.1.6",
    "ui5-tooling-modules": "^3.29.0",
    "ui5-tooling-transpile": "^3.3.7",
    "ui5-task-zipper": "^3.4.x",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "ui5": {
    "dependencies": [
      "ui5-tooling-modules",
      "three",
      "@types/thre",
      "qunit",
      "@sapui5/type"
    ]
  },
  "scripts": {
    "start": "fiori run --open \"test/flp.html#app-preview\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flp.html#app-preview\"",
    "build": "ui5 build self-contained --all --config=ui5.yaml --clean-dest --dest dist",
    "lint": "eslint ./",
    "ts-typecheck": "tsc --noEmit --skipLibCheck",
    "ts-trace": "tsc --traceResolution > traceResolution.log",
    "prestart": "npm run ts-typecheck",
    "prebuild": "npm run ts-typecheck",
    "deploy": "fiori cfDeploy",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "int-test": "fiori run --open 'test/integration/opaTests.qunit.html'",
    "start-variants-management": "fiori run --open \"preview.html#app-preview\"",
    "unit-test": "fiori run --open 'test/unit/unitTests.qunit.html'",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "build:mta": "rimraf resources mta_archives && mbt build",
    "undeploy": "cf undeploy zsapbtpdecp --delete-services --delete-service-keys --delete-service-brokers"
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
